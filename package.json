{
  "name": "app_boil",
  "version": "1.0.0",
  "description": "basic react redux boilerplate",
  "main": "index.js",
  "scripts": {
    "start-dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.dev.config.js --hot --history-api-fallback --host 0.0.0.0 --port 7070",
    "build-dev": "webpack --config webpack.dev.config.js",
    "build-prod": "webpack --config webpack.prod.config.js",
    "lint": "./node_modules/.bin/eslint src/**/*.js --ignore-pattern node_modules/",
    "test": "jest --verbose",
    "test-watch": "jest --verbose --watch",
    "serve": "node server.js"
  },
  "babel": {
    "plugins": [
      "react-hot-loader/babel"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "author": "James Rutledge",
  "contributors": [
    {
      "name": "James Rutledge",
      "email": "jarutledge2@gmail.com"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.3.0",
    "ejs": "^2.7.4",
    "ejs-loader": "^0.3.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^7.32.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-jest": "^24.4.1",
    "eslint-plugin-react": "^7.25.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.3.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^2.5.3",
    "node-sass": "^4.14.1",
    "postcss-loader": "^6.1.1",
    "react-hot-loader": "^4.12.18",
    "react-test-renderer": "^16.12.0",
    "redux-logger": "^3.0.6",
    "sass-loader": "^12.1.0",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "connect-history-api-fallback": "^1.5.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "global": "^4.3.2",
    "prop-types": "^15.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "~(.*)$": "<rootDir>/src/$1"
    },
    "testURL": "http://localhost"
  }
}
